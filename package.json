{
  "name": "sfdx-npm",
  "description": "Add modules to your Salesforce projects with NPM.",
  "version": "0.0.26",
  "author": "Luke Kozakewycz",
  "bugs": "https://github.com/LukeKoz/sfdx-npm/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2",
    "child_process": "^1.0.2",
    "path": "^0.12.7",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^14",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/LukeKoz/sfdx-npm",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "npm": {
        "description": "Install Salesforce modules using NPM."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "LukeKoz/sfdx-npm",
  "scripts": {
    "clean": "rimraf .oclif.manifest.json; rimraf yarn-error.log; rimraf package-lock.json",
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpublish": "npm run clean",
    "posttest": "tslint -p test -t stylish",
    "prepublishOnly": "npm run clean && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
